# Force exact peer dependency resolution
legacy-peer-deps=false
strict-peer-deps=false

# Auto-install peer dependencies when possible
auto-install-peers=false

# Use exact versions to avoid conflicts
save-exact=false

# Enable strict SSL for security
strict-ssl=true

# Reduce log noise
loglevel=warn

# Force npm to use the specified resolution
fund=false

# Force npm to resolve all conflicts
force=false
